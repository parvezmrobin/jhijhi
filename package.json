    "axios": "^0.18.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.5",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "connect-mongo": "^3.1.0",
    "cookie-parser": "^1.4.4",
