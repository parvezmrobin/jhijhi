{
  "name": "jhijhi",
  "version": "1.0.0",
  "description": "A simple react app backed by Express JS to maintain cricket score",
  "homepage": "https://jhijhi.herokuapp.com",
  "keywords": [
    "crikect",
    "scoring",
    "react",
    "express",
    "jhijhi",
    "custom-scoring",
    "cricket-scoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/parvezmrobin/jhijhi.git"
  },
  "main": "src/server/bin/www",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/bin/www",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha --timeout 20000 src/server/test",
    "seed": "node src/server/console/seed.js",
    "del": "node src/server/console/del.js",
    "postbuild": "node src/server/console/postbuild.js"
  },
  "author": {
    "name": "Parvez M Robin",
    "email": "parvezmrobin@gmail.com",
    "url": "https://parvezmrobin.github.io"
  },
  "license": "ISC",
