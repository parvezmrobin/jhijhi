    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "feather-icons": "^4.21.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-errors": "latest",
    "jquery": "^3.3.1",
