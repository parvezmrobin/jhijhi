{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:promise/recommended"],
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "prefer-destructuring": ["error", {
      "array": false,
      "object": true
    }]
  }
}
